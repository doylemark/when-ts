{
  "name": "say-when",
  "version": "1.0.0",
  "description": "A natural language date/time parser with pluggable rules and multi-language support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rules/en": {
      "import": "./dist/rules/en/index.js",
      "types": "./dist/rules/en/index.d.ts"
    },
    "./rules/br": {
      "import": "./dist/rules/br/index.js",
      "types": "./dist/rules/br/index.d.ts"
    },
    "./rules/nl": {
      "import": "./dist/rules/nl/index.js",
      "types": "./dist/rules/nl/index.d.ts"
    },
    "./rules/ru": {
      "import": "./dist/rules/ru/index.js",
      "types": "./dist/rules/ru/index.d.ts"
    },
    "./rules/zh": {
      "import": "./dist/rules/zh/index.js",
      "types": "./dist/rules/zh/index.d.ts"
    },
    "./rules/common": {
      "import": "./dist/rules/common/index.js",
      "types": "./dist/rules/common/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:types && bun run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "bun build src/index.ts src/rules/en/index.ts src/rules/br/index.ts src/rules/nl/index.ts src/rules/ru/index.ts src/rules/zh/index.ts src/rules/common/index.ts --outdir dist --minify --sourcemap=linked --target=node --splitting",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "date",
    "time",
    "parser",
    "natural-language",
    "nlp",
    "datetime",
    "when",
    "relative-date",
    "date-parser"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markwylde/when.git"
  },
  "bugs": {
    "url": "https://github.com/markwylde/when/issues"
  },
  "homepage": "https://github.com/markwylde/when#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  }
}
